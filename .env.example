# Copy to .env and fill in values; do not commit .env

# LLM providers (choose one or more)
GROQ_API_KEY=
TOGETHER_API_KEY=
FIREWORKS_API_KEY=
OPENROUTER_API_KEY=
OPENAI_API_KEY=
GEMINI_API_KEY=
LLM_PROVIDER=gemini  # options: gemini, openai; defaults to echo for POC if empty
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-1.5-flash
LLM_TEMPERATURE=0.2
LLM_MAX_TOKENS=512

# Self-check thresholds
SELF_CHECK_MIN_GROUNDEDNESS=0.7
SELF_CHECK_RETRY=true

# Vector DB (Qdrant Cloud)
QDRANT_URL=
QDRANT_API_KEY=
QDRANT_COLLECTION=agentic_rag_poc

# App
APP_ENV=dev
LOG_LEVEL=INFO


